{
	"compilerOptions": {
		"target": "ES2019",
		"module": "ES2015",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"outDir": "dist",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"typeAcquisition": {
		"enable": true
	}
}
